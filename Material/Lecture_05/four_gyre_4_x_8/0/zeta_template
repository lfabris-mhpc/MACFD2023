{
    upper
    {
        type            fixedValue;
        value           uniform 0;
    }

    fixedWalls1
    {
        type            fixedValue;
        value           uniform 0;
    }


    fixedWalls2
    {
        type            fixedValue;
        value           uniform 0;
    }

    fixedWalls3
    {
        type            fixedValue;
        value           uniform 0;
    }


    frontAndBack
    {
        type            empty;
    }
}


