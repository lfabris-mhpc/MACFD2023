{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setExprFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

expressions
(
    zeta
    {
        field           zeta;
        dimensions      [0 0 -1 0 0 0 0];

        expression
        #{
           pos().y()
        #};
    }
);


